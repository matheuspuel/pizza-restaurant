{
  "expo": {
    "name": "Pizza Restaurant",
    "slug": "pizza-restaurant",
    "version": "1.0.0",
    "owner": "matheuspuel",
    "privacy": "public",
    "description": "Example app for a pizza restaurant",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "androidStatusBar": {
      "barStyle": "light-content",
      "backgroundColor": "#45100500",
      "translucent": true
    },
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#451005"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#451005"
      },
      "package": "com.matheuspuel.pizzarestaurant"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
